{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/odata/v4/?(.*)$",
      "target": "/odata/v4/$1",
      "destination": "srv-api",
      "authenticationType": "ias",
      "csrfProtection": true
    },
    {
      "source": "^/clients/?(.*)$",
      "target": "/odata/v4/clients/$1",
      "destination": "srv-api",
      "authenticationType": "ias",
      "csrfProtection": true
    },
    {
      "source": "^/api/(.*)$",
      "target": "/api/$1",
      "destination": "srv-api",
      "authenticationType": "ias",
      "csrfProtection": true
    },
    {
      "source":  "^/health$",
      "target": "/health",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/health/(.*)$",
      "target": "/health/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source":  "^/(.*)$",
      "target": "/$1",
      "destination":  "ui5-hr-admin",
      "authenticationType": "ias"
    }
  ]
}
