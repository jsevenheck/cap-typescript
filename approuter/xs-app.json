{
  "welcomeFile": "/",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/v4/(.*) $",
      "target": "/odata/v4/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/api/(. *)$",
      "target": "/api/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source":  "^/health$",
      "target": "/health",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source":  "^/(. *)$",
      "target": "/$1",
      "destination":  "ui5-hr-admin",
      "authenticationType": "none"
    }
  ]
}