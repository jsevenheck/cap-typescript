{
  "name": "cap-srv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run ams:generate && tsc --project tsconfig.json",
    "watch": "npm run ams:generate && concurrently \"tsc --project tsconfig.json --watch --preserveWatchOutput\" \"cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=1 NODE_OPTIONS=--require=ts-node/register cds watch\"",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "lint": "eslint \"**/*.ts\"",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=1 jest --coverage",
    "ams:generate": "node ./scripts/generate-ams.mjs"
  },
  "dependencies": {
    "@cap-js/cds-types": "^0.15.0",
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^3.23.0",
    "@sap-cloud-sdk/http-client": "^3.23.0",
    "@sap/ams": "^3",
    "@sap/audit-logging": "^6.9.0",
    "@sap/cds": "^9.6.1",
    "@sap/logging": "^8.2.0",
    "@sap/xsenv": "^5.3.0",
    "@sap/xssec": "^4.11.1",
    "axios": "^1.13.2",
    "cockatiel": "^3.2.1",
    "node-cron": "^3.0.3",
    "p-limit": "^5.0.0",
    "prom-client": "^15.1.0",
    "redis": "^5.10.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.4.1",
    "@cap-js/sqlite": "^2.1.2",
    "@eslint/eslintrc": "^3.1.0",
    "@sap/ams-dev": "^2",
    "@sap/cds-dk": "^9.6.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.6.3"
  }
}
